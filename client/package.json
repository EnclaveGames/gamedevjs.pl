{
  "name": "Gamedevjs.pl web client",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev babel-node -- ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --hot --inline --history-api-fallback --open --config config/webpack/dev.js",
    "build": "cross-env NODE_ENV=production babel-node -- ./node_modules/webpack/bin/webpack.js -p --progress --config config/webpack/production.js"
  },
  "engines": {
    "node": ">=8.0",
    "npm": ">=3.10"
  },
  "browserslist": [
    ">1%",
    "last 2 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "stage-2",
      "react"
    ],
    "env": {
      "test": {
        "plugins": [
          "rewire"
        ]
      }
    },
    "ignore": [
      "/node_modules/",
      "/build/chunks/",
      "/build/public/",
      "/build/updates/",
      "/build/server.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.32",
    "@babel/preset-env": "^7.0.0-beta.32",
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "*",
    "babel-cli": "^7.0.0-beta.3",
    "babel-core": "^7.0.0-beta.3",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.3.0-beta.8",
    "babel-loader": "^7.1.2",
    "babel-node": "^7.0.0-beta.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^7.0.0-beta.3",
    "babel-preset-react": "^7.0.0-beta.3",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-2": "^7.0.0-beta.3",
    "babel-register": "^7.0.0-beta.3",
    "babel-template": "^7.0.0-beta.3",
    "babel-types": "^7.0.0-beta.3",
    "clean-webpack-plugin": "^0.1.17",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "css-mqpacker": "^6.0.1",
    "eslint": "*",
    "eslint-config-airbnb": "*",
    "eslint-config-prettier": "*",
    "eslint-import-resolver-node": "*",
    "eslint-loader": "*",
    "eslint-plugin-css-modules": "*",
    "eslint-plugin-flowtype": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-jsx-a11y": "*",
    "eslint-plugin-prettier": "*",
    "eslint-plugin-react": "*",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "*",
    "html-webpack-plugin": "^2.30.1",
    "mobx": "^3.3.2",
    "mobx-react": "^4.3.5",
    "node-sass": "^4.5.3",
    "postcss": "*",
    "postcss-loader": "^2.0.8",
    "prerender-spa-plugin": "^2.1.0",
    "raw-loader": "^0.5.1",
    "react-error-overlay": "*",
    "react-test-renderer": "*",
    "sass-loader": "^6.0.6",
    "sass-resources-loader": "*",
    "script-ext-html-webpack-plugin": "^1.8.8",
    "style-loader": "^0.18.2",
    "sw-precache-webpack-plugin": "^0.11.4",
    "uglify-es": "^3.1.9",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-middleware": "*",
    "webpack-dev-server": "^3.10.2",
    "webpack-hot-middleware": "*",
    "webpack-merge": "^4.1.1",
    "webpack-node-externals": "*",
    "workbox-webpack-plugin": "^2.1.2"
  },
  "dependencies": {
    "fecha": "^2.3.1",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
    "react": "^16.0.0",
    "react-animate-height": "*",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scroll": "*",
    "react-transition-group": "*",
    "shallowequal": "^1.0.2"
  }
}
